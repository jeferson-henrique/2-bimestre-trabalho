#include <stdio.h>

int main(){

int a = 10, b = 20, c;

printf ("\n\to valor de A é %d\n  ", a);

printf ("\n\to valor de B é %d\n  ", b);

c = a;
a = b;
b = c;

printf ("\n\tseu valor A após as alteraçoes propostas vale %d\n", a);
printf ("\n\tseu valor B após as alteraçoes propostas vale  %d\n", b);




return 0;

}
