#include <stdio.h>

int main(){

int a, m, d, r;

printf("\n\t quantos anos voce tem ");
scanf("%d",& a);
printf("\n\t quantos meses voce tem ");
scanf("%d",& m);
printf("\n\t quantos dias voce tem ");
scanf("%d",& d);

a = a * 365;
m = m * 30;
r = d + a + m;

printf("\n\tvoce ja viveu %d dias", r);

return 0;

}
